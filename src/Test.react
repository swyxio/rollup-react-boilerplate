    cursor:pointer;
    color:#ffffff;
    font-family:Arial;
    font-size:17px;
    padding:16px 31px;
    text-decoration:none;
    text-shadow:0px 1px 0px #2f6627;
  }
  button:active {
    position:relative;
    top:1px;
  }
`


export default () => {
  return <button onClick={() => $count++}>
  styled-jsx works! {$count}
</button>
}


